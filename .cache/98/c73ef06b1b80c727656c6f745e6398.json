\n      else\r\n            $('.cardValue').show();\r\n  });\r\n});*/\r\n\r\n\r\n\r\n\r\n\r\n/*const randomiseCards= () => {\r\n    \r\n        var deck = cardList;\r\n        function shuffle(array){\r\n            array.sort(()=> Math.random() - 0.5);\r\n        }\r\n        deck = cards.cardList.slice();\r\n        shuffle(deck);\r\n    */"},"lineCount":null}},"error":null,"hash":"96022341a42e36154d6195ca36dc3ef0","cacheData":{"env":{}}}